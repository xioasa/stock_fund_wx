.tabbar {
  position: fixed;
  bottom: 0;
  width: 100%;
  left: 0;
  right: 0;
}

.con {
  display: flex;
  position: relative;
  z-index: 500;
  background-color: #F7F7F7;
}

.con::before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid rgba(0,0,0,0.1);
  color: rgba(0,0,0,0.1);
}

.tab {
  padding: 20rpx 0;
  text-align: center;
  color: #333333;
}

.cut {
  flex: 1;
}

.all {
  width: 30%;
  flex-shrink: 0;
}
.config {
  width: 30%;
  flex-shrink: 0;
}

.active {
  color: #2873ff;
}
